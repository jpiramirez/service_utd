      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

	<model name="ciudad">
      <pose>0 0 0  0 0 0</pose>
      <static>true</static>
      <link name="body">
        <visual name="visual">
          <geometry>
            <mesh><uri>file:///home/servicelab/city.dae</uri></mesh>
          </geometry>
        </visual>
		<collision name="visual">
          <geometry>
            <mesh><uri>file:///home/servicelab/city.dae</uri></mesh>
          </geometry>
        </collision>
      </link>
    </model>
  </world>
</sdf>
